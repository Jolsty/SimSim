<%
dateFormat = function(date) {
    let data = date.split("-");
    let year = data[0];
    let month = data[1];
    let day = data[2].substring(0,2);
    let time = data[2].substring(3).split(":");
    let hour = time[0];
    let minute = time[1];
    let second = time[2];
    if (hour && minute && second) return "" + day + "-" + month + "-" + year + " " + hour + ":" + minute + ":" + second;
    else return "" + day + "-" + month + "-" + year;
}
%>
